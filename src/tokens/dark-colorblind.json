    "button.background": "{palette.brand.blueMuted}",
    "button.hoverBackground": "{palette.brand.blue}",
    "progressBar.background": "{palette.brand.blue}",
    "charts.green": "{palette.accent.success}",
    "debugIcon.startForeground": "{palette.accent.success}",
