{
  "name": "valley-of-bowser-theme",
  "displayName": "Valley of Bowser Theme",
  "description": "A deep dark theme for VS Code",
  "version": "0.2.2",
  "publisher": "CaioDeoli",
  "license": "MIT",
  "type": "module",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaioDeoli/valley-of-bowser-theme"
  },
  "bugs": {
    "url": "https://github.com/CaioDeoli/valley-of-bowser-theme/issues"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "dark",
    "dimmed",
    "high-contrast",
    "protanopia",
    "colorblind"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Valley of Bowser",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Valley of Bowser Dimmed",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-dimmed.json"
      }
    ]
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js && mkdir -p build",
    "tokens:sync-dark": "node src/sync-dark-tokens.js",
    "tokens:extract": "node src/extract-tokens.js",
    "package": "vsce package -o ./build/github-vscode-theme.vsix",
    "prepublishOnly": "npm run build && vsce publish",
    "release": "changeset publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "chroma-js": "^3.2.0",
    "color": "^5.0.3",
    "nodemon": "^3.1.11",
    "vsce": "^2.15.0"
  }
}
